                                            <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                            <rect x="3" y="4" width="18" height="8" rx="3"></rect>
                                            <rect x="3" y="12" width="18" height="8" rx="3"></rect>
                                            <line x1="7" y1="8" x2="7" y2="8.01"></line>
                                            <line x1="7" y1="16" x2="7" y2="16.01"></line>
                                            <path d="M11 8h6"></path>
                                            <path d="M11 16h6"></path>
                                        </svg>
                                    </span>
                                    <span class="nav-link-title">
                                        Clusters
                                    </span>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-area="" asp-controller="Tenants" asp-action="Index">
                                    <span class="nav-link-icon d-md-none d-lg-inline-block">

                                        <!-- Download SVG icon from http://tabler-icons.io/i/checkbox -->
                                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-brand-codepen" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
